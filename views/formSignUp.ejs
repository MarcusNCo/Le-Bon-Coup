    <!-- lastname input -->
    <div class="inputFormConnect form-outline mb-3">
    <label class="form-label" for="form2Example1">Nom</label>
        <input name="lastname" type="text" id="form2Example1" class="form-control" />
    </div>

    <!-- Firstname input -->
    <div class="inputFormConnect form-outline mb-3">
